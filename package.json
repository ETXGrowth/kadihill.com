{
  "name": "kadihill.com",
  "version": "1.0.0",
  "description": "kadihill.com website",
  "author": "Nathaniel Hill <nata@goguna.com>",
  "license": "UNLICENSED",
  "keywords": [],
  "scripts": {
    "start": "next",
    "build": "next build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "healthier"
    ]
  },
  "prettier": {
    "jsxSingleQuote": true,
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      "preval"
    ]
  },
  "dependencies": {
    "@mdx-js/loader": "1.5.1",
    "@next/mdx": "9.1.1",
    "babel-plugin-preval": "3.0.1",
    "next": "9.1.1",
    "next-compose-plugins": "2.2.0",
    "next-offline": "4.0.6",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "remark-emoji": "2.0.2"
  },
  "devDependencies": {
    "healthier": "3.2.0",
    "husky": "3.0.9",
    "lint-staged": "9.4.2",
    "prettier": "1.18.2"
  }
}
