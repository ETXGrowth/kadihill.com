{
  "name": "kadihill.com",
  "version": "1.0.0",
  "description": "kadihill.com website",
  "author": "Nathaniel Hill <hello@nathanielhill.io>",
  "license": "UNLICENSED",
  "keywords": [],
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build && node .next/serverless/scripts/build-rss.js",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\" --ignore-path .gitignore",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect",
    "test": "snyk test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "healthier"
    ]
  },
  "prettier": {
    "jsxSingleQuote": true,
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      "preval"
    ]
  },
  "dependencies": {
    "@mdx-js/loader": "^1.6.4",
    "@next/mdx": "^10.0.7",
    "babel-plugin-preval": "^5.0.0",
    "next": "^10.0.7",
    "next-compose-plugins": "^2.2.0",
    "next-offline": "^5.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "remark-emoji": "^2.1.0",
    "scrollprogress": "^3.0.2",
    "snyk": "^1.324.0",
    "webpack": "^5.23.0"
  },
  "devDependencies": {
    "healthier": "^4.0.0",
    "husky": "^5.0.9",
    "lint-staged": "^10.2.6",
    "prettier": "^2.0.5"
  },
  "snyk": true,
  "volta": {
    "node": "14.15.5",
    "yarn": "1.22.10"
  }
}
