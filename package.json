{
  "name": "kadihill.com",
  "description": "kadihill.com website",
  "author": "Nathaniel Hill <hello@nathanielhill.io>",
  "license": "UNLICENSED",
  "keywords": [],
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\" --ignore-path .gitignore"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,md,mdx,css,html,yml,yaml,scss,sass}": [
      "next lint",
      "prettier --write"
    ]
  },
  "prettier": {
    "jsxSingleQuote": true,
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "dayjs": "^1.10.7",
    "graphql": "^16.2.0",
    "next": "^11.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "scrollprogress": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^16.7.10",
    "@types/react": "^17.0.2",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "typescript": "^4.4.2"
  }
}
