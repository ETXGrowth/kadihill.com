{
  "name": "kadihill.com",
  "description": "kadihill.com website",
  "author": "Nathaniel Hill <hello@nathanielhill.io>",
  "license": "UNLICENSED",
  "keywords": [],
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\" --ignore-path .gitignore"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,md,mdx,css,html,yml,yaml,scss,sass}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "jsxSingleQuote": true,
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@zeit/react-jsx-parser": "^2.0.0",
    "async-sema": "^3.1.0",
    "github-slugger": "^1.3.0",
    "next": "^11.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "scrollprogress": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^16.7.10",
    "@types/react": "^17.0.2",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "typescript": "^4.4.2"
  }
}
