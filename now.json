{
  "name": "kadihill",
  "alias": "kadihill.com",
  "version": 2,
  "regions": [ "all" ],
  "builds": [ { "src": "package.json", "use": "@now/static-build" } ],
  "routes": [
    { "headers": { "cache-control": "max-age=31536000, s-maxage=31536000" },
      "src": "^/$", "dest": "/index.html" },
    { "headers": { "cache-control": "max-age=31536000, s-maxage=31536000" },
      "src": "^/about$", "dest": "/about/index.html" },
    { "headers": { "cache-control": "max-age=31536000, s-maxage=31536000" },
      "src": "^/blog$", "dest": "/blog/index.html" },
    { "headers": { "cache-control": "max-age=31536000, s-maxage=31536000" },
      "src": "^/blog/([^/]+)", "dest": "/blog/$1" },
    { "headers": { "cache-control": "max-age=31536000, s-maxage=31536000" },
      "src": "^/contact$", "dest": "/contact/index.html" },
    { "headers": { "cache-control": "max-age=31536000, s-maxage=31536000" },
      "src": "^/(.*)", "dest": "/404/index.html" }
  ]
}
